   <!-- Header_style5 tpl start-->
<script type="text/j-template" id="tpl_diy_con_typeHeader_style11">
	<div class="Header_style_panel" style="background-image:url(<#= content.bg #>)">
		<div class="membersbox">
			<# if(content.dataset.length){#>
			<div class="mobile11_nav">	
				<ul>
					<# _.each(content.dataset,function(item){ #>
					<li>
						<a href="<#= item.link #>">
							<img src="<#= item.pic #>" width="23" height="21">
							<b><#= item.showtitle #></b>
						</a>
					</li>
					<# }) #>
				</ul>
			</div>
			<# } #>	
		</div>
	</div>
</script>


